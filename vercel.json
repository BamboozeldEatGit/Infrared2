{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["repos/**/*", "public/**/*"]
      }
    }
  ],
  "routes": [
    {
      "src": "/doge/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/interstellar/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 