{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/doge",
      "dest": "/api/proxy.js?type=doge"
    },
    {
      "src": "/interstellar",
      "dest": "/api/proxy.js?type=interstellar"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "installCommand": "npm install",
  "buildCommand": "npm run setup"
} 